<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Automation Suite (ExtentReports for Reporting with ITestListeners)">
	<listeners>
		<listener class-name="listeners.ReportListeners" />
	</listeners>
	<test name="Functional Test">
		<groups>
			<run>
				<include name="Sanity"></include>
				<include name="Regression"></include>
			</run>
		</groups>
		<classes>
			<class name="tests.LoginTest" />
			<class name="tests.LogoutTest" />
		</classes>
	</test> <!-- Regression Test -->
</suite> <!-- Automation Suite -->

